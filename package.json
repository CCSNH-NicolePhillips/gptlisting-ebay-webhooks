{
  "name": "gptlisting-ebay-webhooks",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "netlify dev",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "cli": "tsx src/cli.ts",
    "lint": "eslint --cache --max-warnings=0 --ext .ts,.tsx,.js netlify/functions src",
    "lint:fix": "eslint --cache --fix --ext .ts,.tsx,.js netlify/functions src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier -w netlify/functions src",
    "format:check": "prettier --check netlify/functions src",
    "smoke:sh": "bash scripts/smoke.sh",
    "smoke:ps": "pwsh -File scripts/smoke.ps1",
    "prepare": "husky install",
    "deploy": "netlify deploy --prod",
    "pairing": "tsx scripts/run-pairing.ts analysis.json pairing.json",
    "canary": "tsx scripts/canary-test.ts",
    "verify:golden": "tsx scripts/verify-golden.ts",
    "metrics:print": "tsx scripts/metrics-print.ts",
    "test": "npm run typecheck && jest",
    "test:watch": "jest --watch",
    "test:coverage": "npm run typecheck && jest --coverage",
    "test:verbose": "npm run typecheck && jest --verbose",
    "test:direct-pairing": "tsx scripts/test-direct-pairing.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.21.1",
    "@aws-sdk/client-s3": "^3.931.0",
    "@aws-sdk/s3-request-presigner": "^3.931.0",
    "@google/generative-ai": "^0.7.1",
    "@netlify/blobs": "^6.5.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jose": "5.9.3",
    "ml-distance": "^4.0.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.59.0",
    "redis": "^5.9.0",
    "sharp": "^0.33.5",
    "undici": "^6.19.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@netlify/functions": "^2.6.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "esbuild": "^0.23.0",
    "eslint": "^9.11.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "netlify-cli": "^17.0.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.2.5",
    "tsx": "^4.16.2",
    "typescript": "^5.6.3"
  }
}
